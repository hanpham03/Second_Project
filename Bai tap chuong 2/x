def NhapN():
    x = int(input('nhập số nguyên: '))
    return x

def IsSoNguyenTo(n):
    for i in range(1, n):
        if n % i == 0:
            return False
    return True

def main():

    n = NhapN()

    if(IsSoNguyenTo(n)==True):

        print(str(n) + " là số nguyên tố")

    else:

        print(str(n) + " không là số nguyên tố")

main()